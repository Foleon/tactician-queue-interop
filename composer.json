{
  "name": "league/tactician-queue-interop",
  "description": "Queueing command bus plugin for Tactician based on Queue Interop",
  "license": "MIT",
  "keywords": ["queue-interop", "enqueue", "tactician", "command-bus", "message queue"],
  "require": {
    "php": ">=5.6",
    "league/tactician": "^1.0",
    "queue-interop/queue-interop": "^0.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4.0"
  },
  "autoload": {
    "psr-4": {
      "League\\Tactician\\Interop\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
